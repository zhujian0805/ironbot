{
  "slack": {
    "botToken": "xoxb-YOUR-BOT-TOKEN-HERE",
    "appToken": "xapp-1-YOUR-APP-TOKEN-HERE"
  },
  "llmProvider": {
    "provider": "openai",
    "anthropic": {
      "api": "anthropic",
      "apiKey": "sk-your-anthropic-api-key-here",
      "baseUrl": "https://your-anthropic-instance:5000",
      "model": "grok-code-fast-1"
    },
    "openai": {
      "api": "openai",
      "apiKey": "your-azure-openai-api-key-here",
      "baseUrl": "https://your-resource-name.openai.azure.com/openai/v1",
      "model": "gpt-5-mini"
    },
    "google": {
      "api": "google",
      "apiKey": "your-google-api-key-here",
      "baseUrl": "https://generativelanguage.googleapis.com",
      "model": "gemini-2.0-flash"
    },
    "alibaba": {
      "api": "openai",
      "apiKey": "your-alibaba-qwen-api-key-here",
      "baseUrl": "https://dashscope.aliyuncs.com/compatible-mode/openai/v1",
      "model": "qwen-max-latest"
    },
    "anthropicCompatible": {
      "api": "anthropic",
      "apiKey": "your-grok-compatible-api-key-here",
      "baseUrl": "https://your-grok-instance:5000",
      "model": "grok-code-fast-1"
    }
  },
  "skills": {
    "directory": "~/.ironbot/skills"
  },
  "permissions": {
    "file": "./permissions.yaml"
  },
  "logging": {
    "debug": false,
    "level": "INFO"
  },
  "dev": {
    "mode": false
  },
  "cron": {
    "enabled": true,
    "storePath": "~/.ironbot/cron/jobs.json"
  },
  "sessions": {
    "maxHistoryMessages": 12
  },
  "memory": {
    "sessionIndexing": false
  },
  "memorySearch": {
    "enabled": true,
    "vectorWeight": 0.7,
    "textWeight": 0.3,
    "candidateMultiplier": 4,
    "maxResults": 6,
    "minScore": 0.35,
    "sources": ["memory"],
    "crossSessionMemory": true
  },
  "embeddings": {
    "provider": "local",
    "fallback": "none",
    "local": {
      "modelPath": "hf:ggml-org/embeddinggemma-300M-GGUF/embeddinggemma-300M-Q8_0.gguf"
    },
    "openai": {
      "model": "text-embedding-3-small"
    },
    "gemini": {
      "model": "gemini-embedding-001"
    }
  },
  "retry": {
    "maxAttempts": 3,
    "baseDelayMs": 2000,
    "maxDelayMs": 60000,
    "backoffMultiplier": 2.0,
    "jitterMax": 0.1
  },
  "slack_retry": {
    "maxAttempts": 5,
    "baseDelayMs": 15000,
    "maxDelayMs": 300000
  },
  "slack_rate_limit": {
    "enabled": true,
    "requestsPerSecond": 2,
    "burstCapacity": 5,
    "queueSize": 20
  },
  "auto_routing": {
    "enabled": true,
    "confidenceThreshold": 0.5,
    "optOutSkills": []
  },
  "anthropic_timeout_ms": 60000,
  "claude_max_tool_iterations": 100
}

