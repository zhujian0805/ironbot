{
  "slack": {
    "botToken": "<YOUR_SLACK_BOT_TOKEN>",
    "appToken": "<YOUR_SLACK_APP_TOKEN>",
    "threadContextLimit": 15
  },
  "models": {
    "providers": {
      "anthropic": {
        "models": [
          {
            "id": "opus",
            "name": "Claude Opus",
            "cost": {
              "input": 15.0,
              "output": 75.0
            }
          },
          {
            "id": "sonnet",
            "name": "Claude Sonnet",
            "cost": {
              "input": 3.0,
              "output": 15.0
            }
          },
          {
            "id": "haiku",
            "name": "Claude Haiku",
            "cost": {
              "input": 0.8,
              "output": 4.0
            }
          }
        ],
        "api": "anthropic"
      },
      "openai": {
        "models": [
          {
            "id": "gpt-4o",
            "name": "GPT-4 Optimized",
            "cost": {
              "input": 2.5,
              "output": 10.0
            }
          },
          {
            "id": "gpt-4-turbo",
            "name": "GPT-4 Turbo",
            "cost": {
              "input": 10.0,
              "output": 30.0
            }
          }
        ],
        "api": "openai",
        "baseUrl": "https://api.openai.com/v1"
      },
      "moonshot": {
        "models": [
          {
            "id": "kimi-k2-0905-preview",
            "name": "Kimi K2 (Preview)",
            "cost": {
              "input": 1.0,
              "output": 5.0,
              "cacheRead": 0.1,
              "cacheWrite": 2.0
            }
          }
        ],
        "api": "openai",
        "baseUrl": "https://api.moonshot.cn/v1",
        "apiKey": "<YOUR_MOONSHOT_API_KEY>"
      },
      "google": {
        "models": [
          {
            "id": "gemini-2.0-flash",
            "name": "Gemini 2.0 Flash",
            "cost": {
              "input": 0.075,
              "output": 0.3,
              "cacheRead": 0.01875,
              "cacheWrite": 0.075
            }
          }
        ],
        "api": "openai",
        "baseUrl": "https://generativelanguage.googleapis.com/v1beta/openai/",
        "apiKey": "<YOUR_GOOGLE_API_KEY>"
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.ironbot/workspace",
      "compactionMode": "moderate",
      "subagents": {
        "maxConcurrent": 4
      }
    }
  },
  "skills": {
    "directory": ["~/.ironbot/skills"]
  },
  "permissions": {
    "file": "~/.ironbot/permissions.yaml"
  },
  "logging": {
    "debug": false,
    "level": "INFO"
  },
  "dev": {
    "mode": false
  },
  "cron": {
    "enabled": true,
    "storePath": "~/.ironbot/cron/jobs.json"
  },
  "sessions": {
    "maxHistoryMessages": 12
  },
  "memory": {
    "sessionIndexing": false
  },
  "memorySearch": {
    "enabled": true,
    "vectorWeight": 0.7,
    "textWeight": 0.3,
    "candidateMultiplier": 4,
    "maxResults": 6,
    "minScore": 0.35,
    "sources": ["memory"],
    "crossSessionMemory": true
  },
  "embeddings": {
    "provider": "local",
    "fallback": "none",
    "local": {
      "modelPath": "hf:ggml-org/embeddinggemma-300M-GGUF/embeddinggemma-300M-Q8_0.gguf"
    },
    "openai": {
      "model": "text-embedding-3-small"
    },
    "gemini": {
      "model": "gemini-embedding-001"
    }
  },
  "retry": {
    "maxAttempts": 3,
    "baseDelayMs": 2000,
    "maxDelayMs": 60000,
    "backoffMultiplier": 2.0,
    "jitterMax": 0.1
  },
  "slack_retry": {
    "maxAttempts": 5,
    "baseDelayMs": 15000,
    "maxDelayMs": 300000
  },
  "slack_rate_limit": {
    "enabled": true,
    "requestsPerSecond": 2,
    "burstCapacity": 5,
    "queueSize": 20
  },
  "auto_routing": {
    "enabled": true,
    "confidenceThreshold": 0.5,
    "optOutSkills": []
  },
  "anthropic_timeout_ms": 60000,
  "claude_max_tool_iterations": 100
}
